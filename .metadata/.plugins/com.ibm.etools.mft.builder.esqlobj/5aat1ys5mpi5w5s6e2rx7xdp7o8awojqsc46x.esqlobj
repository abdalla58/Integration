CREATE FUNCTION WBIMB_ESQL_SYS_LIB_concat(ref1 CHARACTER, ref2 CHARACTER) RETURNS CHARACTER
BEGIN
	IF ((ref1 IS NULL) AND (ref2 IS NOT NULL)) THEN
		RETURN '' || ref2;
	ELSEIF ((ref1 IS NOT NULL) AND (ref2 IS NULL)) THEN
		RETURN ref1 || '';
	ELSEIF ((ref1 IS NULL) AND (ref2 IS NULL)) THEN
		RETURN '';
	ELSE
		RETURN ref1 || ref2;
	END IF;

END;